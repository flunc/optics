{
  "name": "flunc-optics",
  "version": "0.0.2",
  "description": "Profunctor optics - lenses, prisms, traversals, folds and isos",
  "repository": {
    "type": "git",
    "url": "https://github.com/flunc/optics.git"
  },
  "main": "index.js",
  "scripts": {
    "release-major": "xyz --repo git@github.com:flunc/optics.git --increment major",
    "release-minor": "xyz --repo git@github.com:flunc/optics.git --increment minor",
    "release-patch": "xyz --repo git@github.com:flunc/optics.git --increment patch",
    "test": "npm run test:unit && npm run test:examples",
    "test:unit": "mocha test .",
    "test:examples": "mocha examples"
  },
  "author": "Scott Christopher",
  "license": "MIT",
  "dependencies": {
    "ramda": "0.19.x",
    "ramda-fantasy": "0.5.x"
  },
  "devDependencies": {
    "jsverify": "0.7.x",
    "mocha": "2.3.x",
    "xyz": "0.5.x"
  },
  "files": [
    "index.js",
    "src"
  ]
}
